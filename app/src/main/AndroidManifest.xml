<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dell.c1ean">
    //允许程序读取外部存储权限
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    //允许装载和卸载文件系统权限
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    //允许程序写入外部存储权限
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".Application.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/brand"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/brand"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.LoginActivity" />
        <activity android:name=".Activity.LoginRegisterActivity" />
        <activity android:name=".Activity.RegisterTypeActivity" />
        <activity android:name=".Activity.User.UserMainPageActivity" />
        <activity android:name=".Activity.UserRegisterActivity" />
        <activity android:name=".Activity.WorkerCompanyRegisterActivity" />
        <activity android:name=".Activity.ADMINISTRATOR.AddCompanyActivity" />
        <activity android:name=".Activity.ADMINISTRATOR.AdministratorActivity" />
        <activity android:name=".Activity.ADMINISTRATOR.CompanyManageActivity" />
        <activity android:name=".Activity.ADMINISTRATOR.CompanyItemActivity" />
        <activity android:name=".Activity.Company.CompanyMainPageActivity" />
        <activity android:name=".Activity.Worker.WorkerMainPageActivity" />
        <activity android:name=".Activity.Company.ManageActivityActivity" />
        <activity android:name=".Activity.ActivityDetailPageActivity" />
        <activity android:name=".Activity.User.QuickOrderActivity" />
        <activity android:name=".Activity.User.EvaluationsActivity" />
        <activity android:name=".Activity.User.OrderActivity" />
        <activity android:name=".Activity.User.PayedActivity"/>
        <activity android:name=".Activity.User.ShowLocationActivity"/>
        <activity android:name=".Activity.User.LocationManageActivity"/>
        <activity android:name=".Activity.User.WaitPayActivity"/>
    </application>

</manifest>